        <shape android:shape="rectangle">
            <size android:width="56dp" android:height="56dp"/>
            <solid android:color="#FFFFFFFF"/>
            <stroke android:width="1dp" android:color="#E6E9EE"/>
            <corners android:radius="10dp"/>
        </shape>
    </item>
</selector>
